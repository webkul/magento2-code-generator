define(
    [
        'uiComponent',
        'Magento_Checkout/js/model/payment/renderer-list'
    ],
    function (
        Component,
        rendererList
    ) {
        'use strict';
        rendererList.push(
            {
                type: '%paymentCode%',
                component: '%moduleName%/js/view/payment/method-renderer/%paymentCode%'
            }
        );
        return Component.extend({});
    }
);